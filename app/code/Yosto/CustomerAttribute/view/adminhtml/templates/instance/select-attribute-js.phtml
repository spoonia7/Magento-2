<script>
    require([
        'jquery',
        'mage/template',
        'prototype'
    ], function(jQuery, mageTemplate){

        setSettings = function(urlTemplate, entityTypeElement, attributeIdElement) {
            var url;

            if (!jQuery('#selectattribute_form').valid()) {
                return false;
            }
            entityTypeElement = $F(entityTypeElement);
            attributeIdElement = $F(attributeIdElement);

            url = mageTemplate(urlTemplate, {
                data: {
                    'entity_type_code': entityTypeElement,
                    'attribute_id': attributeIdElement
                }
            });

            setLocation(url);
        };
    });
</script>
